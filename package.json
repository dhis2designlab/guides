        "styled-components": "^4.2.0",
        "typeface-roboto": "^0.0.54"
    },
    "devDependencies": {
        "@dhis2/cli-style": "^3.2.2",
        "babel-plugin-styled-components": "^1.10.0",
        "gatsby-plugin-styled-components": "^3.0.7",
        "gh-pages": "^2.0.1",
        "husky": "^2.3.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/dhis2designlab/guides.git"
    },
    "bugs": {
        "url": "https://github.com/dhis2designlab/guides/issues"
